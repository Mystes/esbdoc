  <mirrors>
    <mirror>
      <id>java.net.2</id>
      <name>java.net Mirror of http://repo1.maven.org/maven2/</name>
      <url>http://download.java.net/maven/2/</url>
      <mirrorOf>java.net</mirrorOf>
    </mirror>
    <mirror>
      <id>L2fprodMaven2Repository.2</id>
      <name> Mirror of http://www.l2fprod.com/maven2 which has been lost</name>
      <url>http://repo1.maven.org/maven2/</url>
      <mirrorOf>L2fprodMaven2Repository</mirrorOf>
    </mirror>
    <mirror>
      <id>typesafe.2</id>
      <mirrorOf>typesafe</mirrorOf>
      <name>BinTray repository, hosts versions of netty, play etc. that have been removed from their primary repositories</name>
      <url>http://dl.bintray.com/typesafe/maven-releases/</url>
    </mirror>
    <mirror>
      <id>maven.java.net-Public</id>
      <mirrorOf>java.net</mirrorOf>
      <name>Central Maven repository, for public releases and snapshots</name>
      <url>https://maven.java.net/content/repositories/public/</url>
    </mirror>
    <mirror>
      <id>maven.java.net-Public.2</id>
      <mirrorOf>Javanet</mirrorOf>
      <name>Central Maven repository, for public releases and snapshots</name>
      <url>https://maven.java.net/content/repositories/public/</url>
    </mirror>
    <mirror>
      <id>maven.java.net-Public.3</id>
      <mirrorOf>Java.net Maven 2</mirrorOf>
      <name>Central Maven repository, for public releases and snapshots</name>
      <url>https://maven.java.net/content/repositories/public/</url>
    </mirror>
    <mirror>
      <id>snmp4j-repo.2</id>
      <mirrorOf>snmp4j-repo</mirrorOf>
      <name>snmp4j-repo</name>
      <url>https://oosnmp.net/dist/release/</url>
    </mirror>
  </mirrors>
  <profiles>

    <profile>
      <id>nexus</id>
      <repositories>
        <repository>
          <id>Snapshots</id>
          <url>https://nexus.mystes.fi:8081/nexus/</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </repository>
        <repository>
          <releases>
            <updatePolicy>daily</updatePolicy>
            <checksumPolicy>ignore</checksumPolicy>
          </releases>
          <id>wso2-nexus</id>
          <url>http://maven.wso2.org/nexus/content/groups/wso2-public/</url>
        </repository>
      </repositories>

      <pluginRepositories>
        <pluginRepository>
          <id>Snapshots</id>
          <url>https://nexus.mystes.fi:8081/nexus/</url>
          <releases>
            <enabled>true</enabled>
          </releases>
          <snapshots>
            <enabled>true</enabled>
          </snapshots>
        </pluginRepository>
        <pluginRepository>
          <releases>
            <updatePolicy>daily</updatePolicy>
            <checksumPolicy>ignore</checksumPolicy>
          </releases>
          <id>wso2-nexus</id>
          <url>http://maven.wso2.org/nexus/content/groups/wso2-public/</url>
        </pluginRepository>
      </pluginRepositories>

    </profile>
  </profiles>
