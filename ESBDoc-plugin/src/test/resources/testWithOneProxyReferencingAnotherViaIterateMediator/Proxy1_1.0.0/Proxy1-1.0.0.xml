<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="Proxy1"
       transports="https,http"
       statistics="disable"
       trace="disable"
       startOnLoad="true">
    <target>
        <inSequence>

            <iterate>
                <target>
                    <endpoint>
                        <address uri="http://localhost:8280/services/Proxy2"/>
                    </endpoint>
                </target>
            </iterate>

            <drop/>
        </inSequence>

        <outSequence/>
    </target>
    <description>
        <purpose>Test ESBDoc with one proxy referencing another proxy via iterate mediator: Proxy 1</purpose>
    </description>
</proxy>
